  <macro name="all kill 2" interrupt="False" loop="False">msg 'All kill';</macro>
  <macro name="auto gheal wand" interrupt="False" loop="False">equipwand 'greater heal' 1;pause 1000;useobject 'righthand';@target! 'self';</macro>
  <macro name="bandage friend" interrupt="False" loop="True">usetype! 0xe21 'any' 'backpack';pause 100;target! 0xa5b4c;pause 10000;</macro>
