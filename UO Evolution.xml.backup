  <macro name="auto attack" interrupt="False" loop="False">@getenemy 'murderer' 'Enemy' 'criminal' 'gray' 'closest';if @findobject 'enemy';// Set the next line to the distance you want to be at for the macro to insist on turning on war mode.;if distance 'enemy' &lt;= 8;warmode 'on';endif;// Set the next line to the distance you want to be at for the macro to insist you needn't be in war mode any longer.;if distance 'enemy' > 12;warmode 'off';endif;autotargetobject! 'enemy';attack! 'enemy';endif;if not @findobject 'enemy';warmode 'off';endif;@clearusequeue;@cancelautotarget;</macro>
